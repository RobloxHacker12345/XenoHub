local function mousething()
    local rs = game:GetService("RunService")
    local plr = game.Players.LocalPlayer
    local mouse = plr:GetMouse()
    local mousepart = workspace.mousePart
    local beam = Instance.new("Beam")
    local Attachment0 = Instance.new("Attachment")
    local Attachment1 = Instance.new("Attachment")
    Attachment0.Parent = workspace.Beam
    Attachment1.Parent = workspace.Beam
    beam.Name = "prediction"
    beam.Parent = Workspace
    beam.Width0 = 2
    beam.Width1 = 2
    beam.CurveSize0 = 2
    bea.CurveSize1 = 2
    beam.Attachment0 = mouse
    beam.Attachment1 = Terrain
    local x, y, z = mouse.Hit.X, mouse.Hit.Y, mouse.Hit.Z
    beam = Attachment0 * Attachement1 / math.cos(x, y, z)
    beam.CFrame = CFrame.new(x, y, z)
    mouse.TargetFilter = beam

    rs.RenderStepped:Connect(mousething)
end

local function hightlight()
    for _, player in ipairs(Players:GetPlayers()) do
        local character = player.Character
    end
    return nil
end

local mousedist = 25
local plr = game.Players.LocalPlayer
local c = plr.Character

local light = Instance.new("Highlight")
light.Adornee = player.character
light.DepthMode = Enum.DepthMode.Occluded
light.FillColor = Color3(255, 255, 255)
light.FillTransparency = 1
light.Name = "highlight"
light.OutlineColor = Color3(255, 255, 255)
light.OutlineTransparency = 0
light.Parent = player.character
c.football = hightlight()

Players.PlayerAdded:Connect(highlight)

for _, player in ipairs(Players:GetPlayers()) do
    local character = player.Character
    if character then
        local football = character:FindFirstChild("Football")
        if football then
            local bbg = Instance.new("BillBoardGui")
            bbg.Text = player.Name
            return player
        end
        return nil
    end
end

