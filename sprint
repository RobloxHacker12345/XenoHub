local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua"))()

Rayfield:Notify({
	Title = "intro",
	Content = "sybau and dig in yo buh",
	Duration = 2,
	Image = 4483362458,
	Actions = {
		Ignore = {
			Name = "hell nah!",
			Callback = function()
				print("The user tapped Okay!")
			end
		},
	},
})

local Window = Rayfield:CreateWindow({
	Name = "idk a window",
	LoadingTitle = "Rayfield Interface Suite",
	LoadingSubtitle = "by kia",
	ConfigurationSaving = {
		Enabled = true,
		FileName = "SprintHub"
	},
	KeySystem = true,
	KeySettings = {
		Title = "hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/sirius)",
		FileName = "SiriusKey",
		SaveKey = true,
		GrabKeyFromSite = false,
		Key = "kia"
	}
})

local Tab = Window:CreateTab("Player", 4483362458)

local spe = false
local sp = 0.03

local Toggle = Tab:CreateToggle({
	Name = "Toggle Sprint",
	CurrentValue = false,
	Flag = "SprintToggle",
	Callback = function(Value)
		spe = Value
	end,
})

local Slider = Tab:CreateSlider({
	Name = "Sprint Speed",
	Range = {0.01, 0.30},
	Increment = 0.01,
	CurrentValue = 0.01,
	Flag = "SprintSpeed",
	Callback = function(Value)
		sp = Value
	end,
})

local Players = game:GetService("Players")
local Input = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")

local player = Players.LocalPlayer
local sprintActive = false
local uiVisible = true

Input.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end

	if input.KeyCode == Enum.KeyCode.ButtonStart then
		VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.RightShift, false, game)
		task.wait(0.05)
		VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.RightShift, false, game)
	end

	if input.KeyCode == Enum.KeyCode.RightShift then
		uiVisible = not uiVisible
		if uiVisible then
			Rayfield:Toggle(true)
		else
			Rayfield:Toggle(false)
		end
	end

Input.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift or input.KeyCode == Enum.KeyCode.ButtonR2 then
		sprintActive = true
	end
end)

Input.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift or input.KeyCode == Enum.KeyCode.ButtonR2 then
		sprintActive = false
	end
end)


RunService.Heartbeat:Connect(function()
	local character = player.Character
	if not character then return end

	local rootPart = character:FindFirstChild("HumanoidRootPart")
	local humanoid = character:FindFirstChildOfClass("Humanoid")
	if not rootPart or not humanoid then return end

	if spe and sprintActive and humanoid.MoveDirection.Magnitude > 0 then
		local offset = humanoid.MoveDirection.Unit * sp
		rootPart.CFrame = rootPart.CFrame + offset
	end
end)
