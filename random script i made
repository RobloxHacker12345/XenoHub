local rf = game.ReplicatedStorage.Remotes
local acr = rf.Kick
if dac then return end

acr.Name = ""
Instance.new("RemoteEvent", rf).Name = "Kick"

dac = true

local tcs = game:GetService("TextChatService")
tcs.OnIncomingMessage = function(msg)

local t = msg.Text:lower()

 local n = msg.TextSource.Name
    local plr = game.Players:FindFirstChild(n)
	local c = plr.Character or plr.CharacterAdded:Wait()
	 if not plr then return end
          
local cmd, args = t:match("^/(%S+)%s*(.*)$")
    if not cmd then return end 
          

    if cmd == "sprintspeed" or "ss" then
        local txt = tonumber(args)
        if txt then 
		 c:SetAttribute("SprintSpeed", txt)
		 end
	  end
	end 
 if cmd == "max" or "m" then
        if txt then 
		 c.Sprint.Stam:SetAttribute("Max", txt)
		 end
      end
	end 
 if cmd == "temp" or "t" then
        if txt then 
		 c.Temperature.Enabled = txt
		 end
      end
	end  
